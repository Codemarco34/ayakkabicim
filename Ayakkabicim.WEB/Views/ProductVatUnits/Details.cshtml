@{
    ViewData["Title"] = "Details";
}
@model ProductVatUnitsDto

<div class="col-lg-12">
    <div class="card">
        <div class="card-body">
           
                <h4 class="card-title">Kdv Oranları</h4>
                <input type="hidden" asp-for="Id" id="ProductVatUnitsIdDetails">

            <div class="mb-3 row">
                <label asp-for="Name" class="col-md-2 col-form-label" for="ProductVatUnitsNameDetails">Kdv Oranı</label>
                <div class="col-md-6">
                    <input id="ProductVatUnitsNameDetails" asp-for="Name" type="text" class="form-control" placeholder="Kdv Oranını Yazınız" disabled>
                    <div class="invalid-feedback">Kdv Oranı Boş olamaz!</div>
                </div>
            </div>

            <div class="mb-3 row">
                <label asp-for="Code" class="col-md-2 col-form-label">Kdv Oran Kodu</label>
                <div class="col-md-6">
                    <input id="ProductVatUnitsCodeDetails" asp-for="Code" type="text" class="form-control" placeholder="Kdv Oran Kodunu Yazınız" disabled>
                    <div class="invalid-feedback">Kdv Oran Kodu Boş olamaz!</div>
                </div>
            </div>


            <div class="mb-3 row">
                <label asp-for="Explanation" class="col-md-2 col-form-label" disabled>Açıklama</label>
                <div class="col-md-6">
                    <textarea id="elm1" asp-for="Explanation" class="form-control"></textarea>

                </div>
            </div>


        </div>
    </div>
</div>

